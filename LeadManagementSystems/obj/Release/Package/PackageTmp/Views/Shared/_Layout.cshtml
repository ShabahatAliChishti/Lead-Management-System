    <script type="text/javascript">
        $(document).ready(function () {
            $.ajax({
                url: "/Base/ViewNotification",
                success: function (data) {
                    console.log(data.data.length);
                    
                    if (data.data.length > 0) {
                        $("#bell").text(data.data.length);
                        $("#notificationBellLinkFromHeader").click(function () {
                            Activelead1();
                        });

                        for (var i = 0; i < data.data.length; i++) {
                            $("#notificationmenu").append('<li><a href="#" onclick="Activelead()"><i class="fa fa-users text-aqua"></i>' + data.data[i].NotificationText + '</a></li>');

                        }
                    }
                    else {
                        $("#notificationmenu").append('<li><a href="#" onclick="Activelead()"><i class="fa fa-users text-aqua"></i>No New Notification</a></li>');
                    }
                }



            });
        })

        function Activelead1() {
            $.ajax({
                url: "/Base/DeleteNotification",
                success: function (code) {
                    console.log(code);
                }



            });          
            $("#bell").text("");
            // window.location.reload();



        }

        function Activelead()
        {
            $.ajax({
                url: "/Base/DeleteNotification",
                success: function (code) {
                    console.log(code);
                    $("#notificationmenu").remove();
                 
                }



            });
            $("#actvleaddashbrd").click();
            $("#bell").text("");
           // window.location.reload();
               
            
           
        }
    </script>


